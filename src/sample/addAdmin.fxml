<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.addAdmin_cnt">
   <children>
      <Button fx:id="home" layoutX="12.0" layoutY="14.0" mnemonicParsing="false" onAction="#Home" prefHeight="26.0" prefWidth="576.0" text="The Super Store" />
      <TextField fx:id="name" layoutX="225.0" layoutY="92.0" promptText="Name" />
      <ComboBox fx:id="role" layoutX="225.0" layoutY="159.0" prefWidth="150.0" promptText="Store/Warehouse" />
      <TextField fx:id="pass" layoutX="225.0" layoutY="126.0" promptText="Password" />
      <Button fx:id="addBtn" layoutX="225.0" layoutY="194.0" mnemonicParsing="false" onAction="#addAdmin" prefHeight="26.0" prefWidth="149.0" text="Add" />
      <ComboBox fx:id="deleteAdmin" layoutX="224.0" layoutY="266.0" prefWidth="150.0" promptText="Choose Admin" />
      <Button fx:id="delBtn" layoutX="224.0" layoutY="300.0" mnemonicParsing="false" onAction="#deleteAdmin" prefHeight="26.0" prefWidth="150.0" text="Delete" />
   </children>
</AnchorPane>
